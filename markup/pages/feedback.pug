extends _template.pug
include ../components/page-header/page-header.pug
include ../components/page-footer/page-footer.pug
include ../components/feedback/feedback.pug
include ../components/modal/modal.pug

block append data
	- const [ activePageData ] = getActivePageData('feedback');


block head
	- pageTitle = 'Sedona';

block content
	| %=symbols=%
	.page__wrapper
		+page-header(activePageData, 'page-header')
		+feedback(feedback, activePageData, 'feedback')
		+page-footer(pageFooter)
		+modal(modal, 'success')
		+modal(modal, 'failure')

block prepend scripts
	script(src='https://cdnjs.cloudflare.com/ajax/libs/hyperform/0.9.22/hyperform.min.js')
