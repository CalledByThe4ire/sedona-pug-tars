extends _template.pug
include ../components/page-header/page-header.pug
include ../components/gallery/gallery.pug
include ../components/page-footer/page-footer.pug

block append data
	- const [ activePageData ] = getActivePageData('gallery');

block head
	- pageTitle = 'Sedona';

block content
	| %=symbols=%
	.page__wrapper
		+page-header(activePageData, 'page-header')
		+gallery(gallery, activePageData, 'gallery')
		+page-footer(pageFooter)
